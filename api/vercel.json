{
  "version": 2,
  "builds": [
    {
      "src": "posts/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "auth/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/posts",
      "dest": "/posts/index.ts"
    },
    {
      "src": "/api/auth",
      "dest": "/auth/index.ts"
    }
  ]
}